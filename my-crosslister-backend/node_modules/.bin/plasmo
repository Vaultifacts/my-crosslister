#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Matt1/OneDrive/Desktop/my-crosslister/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.1_a1f23709aa22305d18e0808c19a97790/node_modules/plasmo/bin/node_modules:/mnt/c/Users/Matt1/OneDrive/Desktop/my-crosslister/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.1_a1f23709aa22305d18e0808c19a97790/node_modules/plasmo/node_modules:/mnt/c/Users/Matt1/OneDrive/Desktop/my-crosslister/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.1_a1f23709aa22305d18e0808c19a97790/node_modules:/mnt/c/Users/Matt1/OneDrive/Desktop/my-crosslister/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Matt1/OneDrive/Desktop/my-crosslister/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.1_a1f23709aa22305d18e0808c19a97790/node_modules/plasmo/bin/node_modules:/mnt/c/Users/Matt1/OneDrive/Desktop/my-crosslister/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.1_a1f23709aa22305d18e0808c19a97790/node_modules/plasmo/node_modules:/mnt/c/Users/Matt1/OneDrive/Desktop/my-crosslister/node_modules/.pnpm/plasmo@0.90.5_@swc+core@1.1_a1f23709aa22305d18e0808c19a97790/node_modules:/mnt/c/Users/Matt1/OneDrive/Desktop/my-crosslister/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../plasmo/bin/index.mjs" "$@"
else
  exec node  "$basedir/../plasmo/bin/index.mjs" "$@"
fi
